<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified">
	<xs:element name="routes">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="route" maxOccurs="unbounded" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element type="xs:string" name="route_long_name"/>
							<xs:element type="xs:byte" name="route_short_name"/>
							<xs:element type="xs:short" name="id"/>
							<xs:element name="stoptimes" maxOccurs="unbounded" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element type="xs:time" name="arrival_times" minOccurs="0"/>
										<xs:element type="xs:byte" name="stop_sequence"/>
										<xs:element type="xs:short" name="stop_id"/>
										<xs:element type="xs:int" name="trip_id"/>
										<xs:element type="xs:int" name="id"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>